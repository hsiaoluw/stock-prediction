python2 train_model.py --predict_type=binary --day=1 >binary_1day.txt
python2 train_model.py --predict_type=binary --day=5 >binary_5day.txt
python2 train_model.py --predict_type=binary --day=20 >binary_20day.txt
python2 train_model.py --predict_type=4class --day=1 >4class_1day.txt
python2 train_model.py --predict_type=4class --day=5 >4class_5day.txt
python2 train_model.py --predict_type=4class --day=20 >4class_20day.txt
